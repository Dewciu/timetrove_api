{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "This is an API for TimeTrove application",
        "title": "TimeTrove API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Kacper Kr√≥l",
